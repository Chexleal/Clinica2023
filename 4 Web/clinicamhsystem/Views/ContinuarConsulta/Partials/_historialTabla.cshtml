@model ConsultaContinuarViewModel
<!-- Modal Crear Paciente-->


<div class="modal fade" id="modalConsulta" tabindex="-1" role="dialog" aria-labelledby="modalConsulta" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title"></h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div id="modalConsultaBody" class="modal-body">
                <div id="loading" class="container-fluid" style="inline-size: fit-content;"><i class="fas fa-circle-notch fa-spin" style="font-size: 40px;"></i></div>
                <div class=" table-responsive ">
                    <table id="table" class="table table-striped ">
                        <thead class="table-dark">
                            <tr>
                                <th>Acciones</th>
                                <th>Fecha</th>
                                <th>Motivo de consulta</th>
                                <th>Historia Clinica</th>
                                <th>Diagnostico</th>
                                <th>Observaciones</th>                   
                            </tr>
                        </thead>
                        <tbody>
                         
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>